[user]
	name = Patrick M. Niedzielski
	email = patrick@pniedzielski.net
	signingkey = DEBFA176
[core]
	editor = emacs -nw
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
	page = true
[alias]
	prettylog = log --oneline --abbrev-commit --decorate --graph
	gpglog = log --oneline --show-signature
[push]
	default = simple
[commit]
	template = ~/.gitmessage
	gpgsign = true
[merge]
	conflictstyle = diff3
	log = true
[clangFormat]
	binary = clang-format-3.7
